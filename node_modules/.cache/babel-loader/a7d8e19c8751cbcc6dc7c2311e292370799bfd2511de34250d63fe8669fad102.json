.indexOf('courses') == -1 || page_addr.indexOf('reviews') == -1) {\n      if (scrollY > 100) {\n        navbarscrolled();\n      }\n      if (scrollY == 0) {\n        navbarnormal();\n        checker();\n      }\n    }\n  });\n  checker();\n}\nwindow.onload = onloadfunction();\nwindow.addEventListener('click', () => {\n  onloadfunction();\n});\nexport default {\n  data() {\n    return {\n      dialogVisible: false\n    };\n  },\n  methods: {}\n};","map":{"version":3,"names":["onloadfunction","navbarelem","document","getElementById","wwnameelem","navbar__btnelem1","navbar__btnelem2","navbar__btnelem4","navbar__btnelem5","page_addr","window","location","href","navbarnormal","style","background","color","className","navbarscrolled","scrolltotop","scrollTo","scrollcheck","scrollY","wwname","onclick","checker","indexOf","addEventListener","onload","data","dialogVisible","methods"],"sources":["D:\\folderforsite\\src\\components\\Navbar.vue"],"sourcesContent":["<template>\r\n    <div id=\"navbar\" class=\"navbar\">\r\n        <div>\r\n        </div>  \r\n        <div id=\"wwname\" class=\"wwname\" @click=\"$router.push('/');sitepath='/'\"><img \r\n        style=\"height: 10vh;width:auto;margin-top:-3vh;\" src=\"logo.png\"\r\n        >\r\n             <div style=\"margin-top:-7vh ;margin-left:9vh ;\">WordWide</div>\r\n        </div>\r\n        <div id=\"navbar__btns\" class=\"navbar__btns\">\r\n            <my-button id=\"navbar__btn1\" style=\"margin-left:0vh\" @click=\"$router.push('/teachers'); \">ПРЕПОДАВАТЕЛИ</my-button>\r\n            <my-button id=\"navbar__btn2\" style=\"margin-left:0vh\" @click=\"$router.push('/courses');\">КУРСЫ</my-button>\r\n            <my-button id=\"navbar__btn5\" style=\"margin-left:0vh\" @click=\"$router.push('/about');\">О НАС</my-button>\r\n        </div>\r\n            <my-button-gray id=\"navbar__btn4\" class=\"appl_btn\" style=\"margin-left:auto\" @click='dialogVisible=true'>ОСТАВИТЬ ЗАЯВКУ</my-button-gray>\r\n    </div>\r\n    <my-dialog v-model:show=\"dialogVisible\">\r\n    </my-dialog>\r\n</template>\r\n\r\n<script language=\"JavaScript\">\r\nfunction onloadfunction(){ \r\n    const navbarelem = document.getElementById(\"navbar\");\r\n    const wwnameelem = document.getElementById(\"wwname\");\r\n    const navbar__btnelem1 = document.getElementById(\"navbar__btn1\");\r\n    const navbar__btnelem2 = document.getElementById(\"navbar__btn2\");\r\n    const navbar__btnelem4 = document.getElementById(\"navbar__btn4\");\r\n    const navbar__btnelem5 = document.getElementById(\"navbar__btn5\");\r\n    let page_addr=window.location.href;\r\n\r\n    function navbarnormal(){ \r\n\r\n        navbarelem.style.background = \"NONE\"\r\n        wwnameelem.style.color = \"white\"\r\n        navbar__btnelem1.className = \"btn\"\r\n        navbar__btnelem2.className = \"btn\"\r\n        navbar__btnelem5.className = \"btn\"\r\n        navbar__btnelem4.className = \"btn_two\"\r\n\r\n    }       \r\n\r\n    function navbarscrolled(){\r\n\r\n        navbarelem.style.background = \"white\"\r\n        wwnameelem.style.color = \"black\"\r\n        navbar__btnelem1.className = \"btnscrolled\"\r\n        navbar__btnelem2.className = \"btnscrolled\"\r\n        navbar__btnelem5.className = \"btnscrolled\"\r\n        navbar__btnelem4.className = \"btn_twoscrolled\"\r\n\r\n    }\r\n    function scrolltotop() {\r\n        window.scrollTo(0, 0);\r\n    }\r\n    function scrollcheck() {\r\n        if (scrollY > 100) {\r\n            navbarnormal()\r\n            scrolltotop()\r\n        }\r\n        if (scrollY == 0){\r\n            navbarnormal()\r\n        }\r\n    }\r\n    \r\n    wwname.onclick = scrollcheck\r\n    navbar__btnelem1.onclick = navbarscrolled\r\n    navbar__btnelem2.onclick = navbarscrolled\r\n    navbar__btnelem5.onclick = navbarscrolled\r\n    navbar__btnelem1.onclick = scrolltotop\r\n    navbar__btnelem2.onclick = scrolltotop\r\n    navbar__btnelem5.onclick = scrolltotop\r\n\r\n    function checker(){\r\n        if (page_addr.indexOf('teachers') > -1 || page_addr.indexOf('about') > -1 || page_addr.indexOf('courses') > -1 || page_addr.indexOf('reviews') > -1)\r\n        {\r\n            \r\n            navbarscrolled()\r\n        }\r\n    }\r\n    window.addEventListener('scroll', () => {\r\n        if (page_addr.indexOf('teachers') == -1 || page_addr.indexOf('about') == -1 || page_addr.indexOf('courses') == -1 || page_addr.indexOf('reviews') == -1)\r\n        {\r\n            \r\n            if (scrollY > 100 ) {\r\n              navbarscrolled()\r\n            }\r\n            if (scrollY == 0 ){\r\n                navbarnormal()\r\n                checker()\r\n            }\r\n        }\r\n       \r\n    })\r\n    checker()\r\n}\r\n\r\nwindow.onload = onloadfunction()\r\n\r\nwindow.addEventListener('click', () => {\r\n        onloadfunction()\r\n})\r\n\r\nexport default {\r\n    data(){\r\n        return{\r\n            dialogVisible:false,\r\n            \r\n        }\r\n    },\r\n    methods:{\r\n    }\r\n}\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n</script>\r\n\r\n\r\n\r\n<style scoped>\r\n.navbar {\r\n    height: 80px;\r\n    background-color: rgba(255, 255, 255, 0);\r\n    display: flex;\r\n    align-items: center;\r\n    padding: 0 15px;\r\n    position:fixed;\r\n    width: 100%;\r\n    transition: background  0.25s;\r\n    z-index: 500;\r\n}\r\n\r\n\r\n.navbar__btns{\r\n    \r\n    margin-top: 1vh;\r\n}\r\n.wwname{\r\n    \r\n    padding: 10px 15px;\r\n    background: none;\r\n    color: rgb(255, 255, 255);\r\n    border: 0px solid rgb(255, 255, 255);\r\n    font-family: 'Comfortaa', sans-serif;\r\n    font-size: 30px;\r\n    margin-left:-1vh;\r\n    letter-spacing: 0.0625em;\r\n    cursor:pointer;\r\n}\r\n.btnscrolled {\r\n    padding: 10px 15px;\r\n    background: none;\r\n    color: rgb(0, 0, 0);\r\n    border: 0px solid rgb(255, 255, 255);\r\n    font-family: 'Comfortaa', sans-serif;\r\n    font-size: 2vh !important;\r\n    -webkit-text-stroke-width: 0